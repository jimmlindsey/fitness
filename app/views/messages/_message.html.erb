<div class="panel">
  <div class="panel-body">
    <%= image_tag message.user.avatar.url, class: "img-circle avatar-medium" %>
    <strong><%= message.user.fullname %></strong>
    <span class="pull-right"><%= message.message_time %></span>
    <br>
    <div class="row-space-2 message">
      <%= message.context %>
    </div>
  </div>
</div>